<div id='header'>
  <div id='header-left-column-container'>
    
  {{#if settings.IS_ENTITLEMENT_VIEWER}}
    <!--
      If API is not available then testing on the
      desktop so show the button, otherwise only
      if this is an entitlement viewer.
    -->
    <div class='text-link' id='print-subscriber-login'>{{model.loginLbl}}</div>
  {{/if}}
  
    <!-- for development only (do not use this in live app) -->
    <div class='text-link' id="go-to-library" style="float:left;margin-top:15px;margin-left:20px;">Library</div>

  {{#if settings.IS_HEADER_TEXT}}
    <div id='title'><span style='margin:0;padding:0;border:0;'>{{settings.IS_HEADER_TEXT}}</span></div>
  {{else}}
    <img id='title-image' src=''>
  {{/if}}
    </div>
  </div>
</div>



